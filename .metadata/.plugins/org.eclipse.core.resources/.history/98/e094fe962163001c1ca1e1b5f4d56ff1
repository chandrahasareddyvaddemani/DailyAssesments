package com.chandra;

public class chan {
	B b;  
	chan(){
		System.out.println("a is created");
		}
	
	public B getB() {  
	    return b;  
	}
	
	public void setB(B b) {  
	    this.b = b;  
	}
	
	void print(){
		System.out.println("hello a");
		}  
	
	public void display(){  
	    print();  
	    b.print();  
	}  
}
