<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- bean definitions here -->
<bean class="Employee" name="emp123" scope="prototype">
<property name="eid" value="123"></property>
<property name="name" value="chanadra"></property>
<property name="laptop" ref="laptop123"></property>
</bean>
<bean class="Laptop" name="lap123" scope="singleton">
<property name="lid" value="123"></property>
<property name="cpu" value="chanadra"></property>
<property name="ram" value="12"></property>


</bean>
</beans>


